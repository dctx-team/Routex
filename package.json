{
  "name": "routex",
  "version": "1.1.0-beta",
  "description": "Next-gen AI API router and load balancer",
  "keywords": [
    "ai",
    "api",
    "router",
    "load-balancer",
    "claude",
    "anthropic",
    "openai",
    "smart-routing",
    "transformers",
    "proxy"
  ],
  "author": "dctx-team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dctx-team/Routex.git"
  },
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "start": "bun run src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "build:dashboard": "cd dashboard && bun run build",
    "dev:dashboard": "cd dashboard && bun run dev",
    "test": "bun test",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "strip-chinese": "bun run scripts/strip-chinese.ts",
    "select-model": "bun run src/cli/model-selector.ts",
    "cli": "bun run src/cli/model-selector.ts"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/bun": "^1.2.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20",
    "bun": ">=1.2.0"
  }
}
